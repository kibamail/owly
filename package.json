    },
    "./alert": {
      "types": "./dist/esm/alert/index.d.ts",
      "import": "./dist/esm/alert/index.js",
      "require": "./dist/cjs/alert/index.js"
    },
    "./heading": {
      "types": "./dist/esm/heading/index.d.ts",
      "import": "./dist/esm/heading/index.js",
      "require": "./dist/cjs/heading/index.js"
